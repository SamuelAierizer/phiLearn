<main class="bg-gray-100 dark:bg-gray-900 h-screen overflow-hidden relative">
  <div class="flex items-start justify-between">
    <!-- Sidenav --> 
    <%= render 'shared/sidenav' %>


    <div class="flex flex-col w-full md:space-y-4">
      <!-- Header -->
      <%= render 'shared/navbar' %>

      <div class="overflow-auto h-screen pb-24 px-4 md:px-6 z-20">
        <!-- Page Head -->
        <div class="my-4">
          <h1 class="text-4xl font-semibold text-gray-800 dark:text-white">Courses overview</h1>
          <h2 class=" text-gray-400"><%= @school.name %></h2>
        </div>

        <!-- Page Conent -->
        <%= render 'courses/table' %>
        
      </div>
    </div>
  </div>
</main>